Class {
	#name : 'Rowan3LoadedClassStub',
	#superclass : 'Object',
	#instVars : [
		'theClass'
	],
	#category : 'Rowan3Stub-Core'
}

{ #category : 'accessing' }
Rowan3LoadedClassStub >> handle [
	^ self theClass
]

{ #category : 'accessing' }
Rowan3LoadedClassStub >> theClass [
	^theClass
]

{ #category : 'accessing' }
Rowan3LoadedClassStub >> theClass: object [
	theClass := object
]
